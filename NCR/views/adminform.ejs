<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <meta charset="utf-8">
  <title>Admin Form</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-F3w7mX95PdgyTmZZMECAngseQB83DfGTowi0iMjiWaeVhAn4FJkqJByhZMI3AhiU" crossorigin="anonymous">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" />
  <link href="http://fonts.cdnfonts.com/css/segoe-ui-4" rel="stylesheet">

  <link rel="stylesheet" href="/css/adminForm.css">
</head>

<body>
  <nav>
    <input type="checkbox" id="check">
    <label for="check" class="checkbtn">
      <i class="fas fa-bars"></i>
    </label>
    <div class="logo-image">
      <img src="/images/logo-3.png" class="logo">
    </div>
    <ul>
      <li><a class="active" href="/adminHome">Home</a></li>
      <li><a href="/dashboard">Dashboard</a></li>
      <li><button class="btn  btn-outline-primary btn-lg" onclick="location.href='/logout'" type="button">
          logout</button></li>
    </ul>
  </nav>
  <div class="ncrtitle">
    <h1 style="font-weight: 900">Create NCR</h1>
  </div>
  <div class="formclass py-3 mb-4">
    <form class="formbox" action="/adminHome/createNCR" enctype="multipart/form-data" method="post">
      <div class="form-group">
        <label for="supplierName">Supplier Name</label>
        <select name="supplierName" class="form-control" id="supplierName">
          <option>Supplier Name</option>
          <% array.forEach((element)=> {%>
            <option value="<%=element.username%>">
              <%=element.username%>
            </option>
          <%});%>
        </select>
      </div>

      <div class="form-group">
        <label for="exampleInputEmail1">Supplier Email</label>
        <input type="email" class="form-control" name="supplierEmail" id="exampleInputEmail1"
          aria-describedby="emailHelp" required>
        <small id="emailHelp" class="form-text text-muted"></small>
      </div>

      <div class="form-group">
        <label for="category">Category of product</label>
        <input type="text" class="form-control" name="category" value="">
      </div>

      <div class="form-group">
        <label for="partName">Part Name</label>
        <input type="text" class="form-control" name="partName" value="">
      </div>

      <div class="form-group">
        <label for="po/jo number">Po No/Jo No</label>
        <input type="text" class="form-control" name="pojo" value="">
      </div>

      <div class="form-group">
        <label for="challanNumber">Challan Number/Invoice No</label>
        <input type="text" class="form-control" name="challanNo" value="">
      </div>

      <div class="form-group">
        <label for="totalRCVD">Total recieved Quantity</label>
        <input type="number" class="form-control" name="totalRCVD" value="" placeholder="Enter Only number" required>
      </div>

      <div class="form-group">
        <label for="NCRquantity">NC Quantity</label>
        <input type="number" class="form-control" name="NCRquantity" value="" placeholder="Enter Only number" required>
      </div>

      <div class="form-group">
        <label for="productDescription">Product Description</label>
        <input type="text" class="form-control" name="productDescription" value="">
      </div>

      <div class="form-group">
        <label for="productDescription">Part Number</label>
        <input type="text" class="form-control" name="partNumber" value="">
      </div>

      <div class="form-group">
        <label for="productName">Product Name</label>
        <input type="text" class="form-control" name="productName" value="">
      </div>

      <div class="form-group">
        <label for="description">Description(NCR)</label>
        <input type="text" class="form-control" name="descriptionncr" value="">
      </div>




      <div class="form-group">
        <label for="exampleFormControlFile1">Product File</label>
        <input type="file" class="form-control-file" id="exampleFormControlFile1" name="uploaded_file" accept='file_extension/*'
          multiple required>
      </div>
     

      <button type="submit" class="btn btn-primary">Submit</button>
    </form>

  </div>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-/bQdsTh/da6pkI1MST/rWKFNjaCP5gBSY4sEBT38Q/9RBh9AH40zEOg7Hlq2THRZ"
    crossorigin="anonymous"></script>
</body>

</html>
