<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Dashboard</title>

  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-F3w7mX95PdgyTmZZMECAngseQB83DfGTowi0iMjiWaeVhAn4FJkqJByhZMI3AhiU" crossorigin="anonymous">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" />
  <link href="http://fonts.cdnfonts.com/css/segoe-ui-4" rel="stylesheet">

  <link rel="stylesheet" href="/css/styles.css">
</head>

<body>
  <nav>
    <input type="checkbox" id="check">
    <label for="check" class="checkbtn">
      <i class="fas fa-bars"></i>
    </label>
    <div class="logo-image">
      <img src="images/logo-3.png" class="logo">
    </div>
    <ul>
      <li><a class="" href="/adminHome">Home</a></li>
      <li><a class="" href="/adminHome/createNCR">Create NCR</a></li>
      <li><button class="btn  btn-primary btn-lg" onclick="location.href='/logout'" type="button">
          logout</button></li>
      <!-- <li><a class="" href="/logout">Logout</a></li> -->
    </ul>
  </nav>

  <div class="welcome">
    <H1>Dashboard</H1>
  </div>
  <div class="table-responsive table-div">
    <!-- <caption>Year 2021-22</caption> -->
    <table class="table table-light table-striped table-sm table-hover table-bordered border-primary">
      <thead class="table-primary">

        <tr>
          <th>Status</th>
          <th>SR NO</th>
          <th>Date</th>
          <th>IO No.</th>
          <th>Supplier</th>
          <th>Catgory</th>
          <th>Part Name</th>
          <th>PO/JO No</th>
          <th>Challan No</th>
          <th>Total Rcvd Qty</th>
          <th>NCR Qty</th>
          <!-- <th>Reason</th> -->
          <th>Immediate Corrective Action</th>
          <th>Root Cause</th>
          <th>Corrective Action</th>


        </tr>
      </thead>

      <tbody>
        <% array4.forEach((element, index) => {%>
        <tr>
          <td><%=element.status%></td>
          <td><%=index +1%></td>
          <td><%=element.date %></td>
          <td><%=element.ioNo%></td>
          <td><%=element.supplierName%></td>
          <td><%=element.category%></td>
          <td><%=element.partName%></td>
          <td><%=element.pojo%></td>
          <td><%=element.challanNo%></td>
          <td><%=element.totalRCVD%></td>
          <td><%=element.NCRQuantity%></td>
          <!-- <td><%=element.reason%></td> -->
          <td><%=element.immediateCorrectiveAction%></td>
          <td><%=element.rootCause1%></td>
          <td><%=element.ca1%></td>


        </tr>
        <%});%>

      </tbody>
    </table>


  </div>
  
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-/bQdsTh/da6pkI1MST/rWKFNjaCP5gBSY4sEBT38Q/9RBh9AH40zEOg7Hlq2THRZ" crossorigin="anonymous"></script>

</body>

</html>
